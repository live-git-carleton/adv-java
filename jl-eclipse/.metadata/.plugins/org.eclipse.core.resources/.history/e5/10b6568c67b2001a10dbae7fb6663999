package multithreading;

public abstract class AbstractClassWithConstructor {
	public AbstractClassWithConstructor() {
		System.out.println("This is abstract class cons!");
	}
}

class Derived extends AbstractClassWithConstructor {
	public static void main(String[] args) {
		new Derived();
	}

	Derived() {
		System.out.println("This is derived cons!");
	}
}